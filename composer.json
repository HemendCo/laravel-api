{
    "name": "hemend/laravel-api",
    "description": "Generate api with service & version & method base",
    "keywords" : [
        "hemend",
        "laravel",
        "api"
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "hemend/php-library": "^1.2.0",
        "laravel/passport": "^12.2.0",
        "propaganistas/laravel-phone": "^5.3.2",
        "spatie/laravel-permission": "^6.7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hemend\\Api\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "balal",
            "email": "arastbalal@gmail.com"
        }
    ],
    "extra" : {
        "laravel" : {
            "providers" : [
                "Hemend\\Api\\ApiServiceProvider"
            ],
            "aliases" : {
                "Api" : "Hemend\\Api\\Facades\\Api"
            }
        }
    }
}
